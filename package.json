{
  "name": "certify-ai",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "client": "cd client && npm start",
    "server": "cd backend && npm run dev",
    "llm": "cd llm && npm run dev",
    "install:client": "cd client && pnpm install",
    "install:server": "cd backend && pnpm install",
    "install:llm": "cd llm && pnpm install",
    "install:all": "pnpm run install:client && pnpm run install:server && pnpm run install:llm",
    "dev": "concurrently -n client,server,llm -c cyan,magenta,yellow \"npm run client\" \"npm run server\" \"npm run llm\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.27.0",
  "dependencies": {
    "concurrently": "^9.2.1"
  }
}
